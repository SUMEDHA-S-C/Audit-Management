@model AuditManagementPortalClientMVC.Models.AuditResponse;

@{ Layout = "_Authenticated"; }

@{ ViewData["Title"] = "SeverityResult"; }

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/page3.css">
</head>

<body>
    @if (Model.ProjectExexutionStatus == "GREEN")
    {
<div class="wrapper">
    <h2 class="title">Audit Response</h2>
    <hr color="#fff">
    <h3>Audit ID:</h3>
    <p>@Model.AuditId </p>
    <h3>Project Execution status:</h3>
    <div id=status></div>
    <h3>Remedial Action Duration :</h3>
    <p>@Model.RemedialActionDuration</p>
</div> 
    }
            else
            {
<div class="wrapper">
    <h2 class="title">Audit Response</h2>
    <hr color="#fff">
    <h3>Audit ID:</h3>
    <p>@Model.AuditId </p>
    <h3>Project Execution status:</h3>
    <div id=status2></div>
    <h3>Remedial Action Duration :</h3>
    <p>@Model.RemedialActionDuration</p>
</div>
    }

</body>




